{
  "name": "tailwind-plus-salient",
  "version": "0.1.0",
  "private": true,
"scripts": {
  "dev": "next dev",
  "build": "next build",
  "start": "next start",
  "lint": "next lint",
  "cards": "concurrently -k \"next dev -p 4000\" \"wait-on http-get://localhost:4000 && node scripts/render-cards.cjs\"",
  "open-cards": "echo 'Generated PNGs:' && ls -1 out/cards && echo '\nTo view, open the Codespaces file browser and navigate to out/cards/'",
  "render:banners": "node scripts/render-banners.mjs",
  "export:banners": "next dev -p 4001 & sleep 3 && RENDER_URL=http://localhost:4001/ads/preview node scripts/render-banners.mjs && pkill -f \"next dev -p 4001\" || true"
}
,
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@headlessui/react": "^2.1.0",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/postcss": "^4.1.7",
    "@types/node": "^20.10.8",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "clsx": "^2.1.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "playwright": "^1.54.2",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sharp": "0.33.1",
    "wait-on": "^8.0.4"
  }
}
